
# $Id: Makefile.in,v 1.4 2002/08/30 13:44:29 mani Exp $

# This line is needed on some machines.
@SET_MAKE@

srcdir = @srcdir@
VPATH = @srcdir@

LIBS=@LIBS@ $(EXTRALIBS)
CC=@CC@
CFLAGS=@CFLAGS@ @DEFS@

all: put

put: put.o
	$(CC) $(CFLAGS) -o put put.o $(LIBS) 

clean:
	rm -f put put.o

spotless:
	-test -f .cvsignore && rm -rf `cat .cvsignore`
